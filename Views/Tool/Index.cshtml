@{
    ViewData["Title"] = "Data Extraction Wizard";
    Layout = "~/Views/Shared/Admin/_AdminLayout.cshtml";
}
<div class="row">
	<div class="col-sm-8"><h4>Data Extraction</h4></div>
	<div class="col-sm-4">
		<button type="button" onclick="location.href='@Url.Action("index", "acttype")'" class="btn btn-info btn-style float-lg-right"><i class="fa fa-arrow-left"></i> Back to list</button>
	</div>
</div>
<hr />
<div class="row">
	<div class=" form-group  text-center offset-3">
		<div class="form-group">
			<label class="control-label">File Type</label>
			<select id="ddlFileType" class="form-control"></select>
		</div>
	</div>
</div>
<div class="row">
	<div class=" form-group  text-center offset-3">
		<div class="form-group">
			<label class="control-label">Url</label>
			<input type="text" id="txtUrl" placeholder="Enter url for capturing data" />
		</div>
	</div>
</div>

